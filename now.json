{
  "version": 2,
  "name": "<%= name %>",
  "builds": [
    {
      "src": "**/*.js",
      "use": "@now/node",
      "config": {
        "maxLambdaSize": "<%= now_max_lambda_size %>"
      }
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/robots.txt", "dest": "/robots.txt" },
    { "src": "/login", "dest": "/login/index.js" },
    { "src": "/api", "dest": "/api/index.js" }
  ]
}
